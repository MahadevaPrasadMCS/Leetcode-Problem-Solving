class Solution(object):
    def twoSum(self, numbers, target):
        """
        :type numbers: List[int]
        :type target: int
        :rtype: List[int]
        """
        diff = {}
        for i,num in enumerate(numbers):
          #Calculate the difference
            sub = target - num
            if sub in diff:
                #return indexes that are represented as +1
                return [diff[sub]+1, i+1]
            #Add number to dictionary is diff not found
            diff[num] = i
